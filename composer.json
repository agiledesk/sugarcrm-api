{
  "name": "troward/sugarapi",
  "description": "SugarCRM 7 API Wrapper",
  "keywords": [
    "sugarcrm",
    "api",
    "wrapper",
    "troward"
  ],
  "license": "MIT",
  "require": {
    "guzzlehttp/guzzle": "^5.2"
  },
  "require-dev": {
    "phpspec/phpspec": "2.0"
  },
  "autoload": {
    "psr-0": {
      "Troward\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "spec\\": ""
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
